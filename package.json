{
  "name": "@davesag/ERC884-reference-implementation",
  "version": "1.0.0",
  "description":
    "An Interface and Reference Implementation of the ERC-884 (DGCL) Token.",
  "main": "truffle.js",
  "directories": {
    "test": "test",
    "contracts": "contracts"
  },
  "engines": {
    "node": "9.10.0",
    "npm": "5.8.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "solhint": "^1.1.10",
    "solidity-coverage": "^0.4.15"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --write '**/*.{js,json}'",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "test": "truffle test",
    "test:cov": "solidity-coverage",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:js": "eslint test/**/*.js"
  },
  "lint-staged": {
    "**/*.{js,json}": ["prettier --singlequote --no-semi --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davesag/ERC884-reference-implementation.git"
  },
  "keywords": ["ERC-20", "Ethereum", "DGCL", "token", "stock"],
  "author": "Dave Sag <davesag@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davesag/ERC884-reference-implementation/issues"
  },
  "homepage":
    "https://github.com/davesag/ERC884-reference-implementation#readme",
  "dependencies": {
    "zeppelin-solidity": "1.8.0"
  }
}
